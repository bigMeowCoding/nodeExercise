<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<h1><%= title %></h1>
<div id="photos">
    <% images.forEach(function (photo) {%>
    <div class="photo">
        <h2>
            <%=photo.name%>
        </h2>
        <a href="/photo/download/<%=photo.id%>">下载</a>
        <img src="/images/<%=photo.path%>" alt="">
    </div>
    <%}) %>

</div>
</body>
</html>
